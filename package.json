{
  "name": "tpc-docs-site",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "npm run re-gen && docusaurus start",
    "build": "npm run re-gen && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "npm run clean-all-api-docs && docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "gen-api-docs": "docusaurus gen-api-docs",
    "clean-api-docs": "docusaurus clean-api-docs",
    "gen-api-docs:version": "docusaurus gen-api-docs:version",
    "clean-api-docs:version": "docusaurus clean-api-docs:version",
    "gen-all-api-docs": "docusaurus gen-api-docs all",
    "clean-all-api-docs": "docusaurus clean-api-docs all",
    "re-gen": "npm run clean-all-api-docs && npm run gen-all-api-docs"
  },
  "dependencies": {
    "@docusaurus/core": "^3.6.1",
    "@docusaurus/preset-classic": "^3.6.1",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "docusaurus-plugin-openapi-docs": "^4.2.0",
    "docusaurus-theme-openapi-docs": "^4.2.0",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.6.1"
  },
  "overrides": {
    "react-live": ">=3.1.1 <4.1.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=22.0"
  }
}
